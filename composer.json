{
    "name": "hiraeth/twig-markdown",
    "description": "Twig Markdown extension for the Hiraeth Nano Framework",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name":  "Matthew J. Sahagian",
            "email": "matthew.sahagian@gmail.com",
            "role":  "Lead Architect / Developer"
        }
    ],
    "require": {
        "hiraeth/app": "^2.0",
        "hiraeth/markdown": "^1.0",
        "hiraeth/markdown-imp": "^1.0",
        "aptoma/twig-markdown": "^2.1"
